{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "projects": {
    "authservice": {
      "type": "application",
      "root": "apps/authservice",
      "entryFile": "main",
      "sourceRoot": "apps/authservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authservice/tsconfig.app.json",
        "webpack": true,
        "deleteOutDir": true
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json",
        "webpack": true,
        "deleteOutDir": true
      }
    },
    "graphql": {
      "type": "library",
      "root": "libs/graphql",
      "entryFile": "index",
      "sourceRoot": "libs/graphql/src",
      "compilerOptions": {
        "tsConfigPath": "libs/graphql/tsconfig.lib.json"
      }
    },
    "orderservice": {
      "type": "application",
      "root": "apps/foodservers/orderservice",
      "entryFile": "main",
      "sourceRoot": "apps/foodservers/orderservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/foodservers/orderservice/tsconfig.app.json",
        "webpack": true,
        "deleteOutDir": true
      }
    },
    "productservice": {
      "type": "application",
      "root": "apps/foodservers/productservice",
      "entryFile": "main",
      "sourceRoot": "apps/foodservers/productservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/foodservers/productservice/tsconfig.app.json",
        "webpack": true,
        "deleteOutDir": true
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "restraurent": {
      "type": "application",
      "root": "apps/foodservers/restraurent",
      "entryFile": "main",
      "sourceRoot": "apps/foodservers/restraurent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/foodservers/restraurent/tsconfig.app.json",
        "webpack": true,
        "deleteOutDir": true
      }
    }
  },
  "compilerOptions": {
    "webpack": true
  }
}